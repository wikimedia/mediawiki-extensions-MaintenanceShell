{
	"name": "MaintenanceShell",
	"version": "0.5.0",
	"author": [
		"[http://swiftlytilting.com Andrew Fitzgerald]",
		"Timo Tijhof"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:MaintenanceShell",
	"descriptionmsg": "maintenanceshell-desc",
	"license-name": "GPL-2.0+",
